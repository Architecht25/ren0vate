{
  "name": "ren0vate",
  "engines": {
    "node": ">=20.17.0 <23",
    "yarn": "1.22.22"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@hotwired/stimulus": "^3.2.2",
    "@hotwired/turbo": "^8.0.13",
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.7",
    "sass": "^1.89.2"
  },
  "scripts": {
    "build": "esbuild app/javascript/*.* --bundle --sourcemap --format=esm --outdir=app/assets/builds --public-path=/assets",
    "build:css": "sass --load-path=node_modules/bootstrap/scss --load-path=node_modules/@fortawesome/fontawesome-free/scss --load-path=node_modules app/assets/stylesheets/application.scss app/assets/builds/application.css",
    "dev:css": "sass --watch --load-path=node_modules app/assets/stylesheets/application.scss:app/assets/builds/application.css"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e",
  "devDependencies": {
    "esbuild": "^0.25.5"
  }
}
